{
    "extends": "airbnb-base",
    "env": {
        "react-native/react-native": true
    },
    "rules": {
        "semi": [
            "error",
            "never"
        ],
        "comma-dangle": [
            "error",
            "only-multiline"
        ],
        "no-unused-vars": 0,
        "no-tabs": 0,
        "class-methods-use-this": 0,
        "no-use-before-define": 0,
        "indent": 0,
        "object-curly-newline": 0
    },
    "plugins": [
        "react",
        "react-native"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    }
}